{
  "name": "pd-shop",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "info": "shopify theme info -e",
    "logout": "shopify auth logout",
    "list": "shopify theme list -e",
    "log": "shopify theme console -e dev",
    "dev": "vite",
    "dev-watch": "shopify theme dev -e dev",
    "dev-shop": "bun run logout && bun run dev-push && bun run dev-watch",
    "dev-push": "shopify theme push -d -e dev",
    "dev-update": "shopify theme push -e prod -d",
    "theme-push": "bun run build && bun run logout && shopify theme push -e dev -n -x='*.json' -t",
    "theme-pull": "bun run logout && shopify theme pull -e prod -l",
    "theme-pull-json-d": "shopify theme pull -e prod -o='*.json' -d",
    "build": "vite build"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.1.3",
    "rollup-plugin-globlin": "^0.1.4",
    "sass": "^1.78.0",
    "vite": "^5.4.3",
    "vite-plugin-shopify": "^3.0.1",
    "vite-plugin-shopify-import-maps": "^0.5.4",
    "@by-association-only/vite-plugin-shopify-clean": "^2.0.0",
    "npm-check-updates": "^17.1.1"
  },
  "dependencies": {
    "motion": "^10.18.0"
  }
}